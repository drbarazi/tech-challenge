{
	"extends": "@tsconfig/react-native/tsconfig.json",
	"compilerOptions": {
	  // "strict": true,
	  "baseUrl": ".",
	  "paths": {
		  "tests": ["tests/*"],
		  "@navigation/*": [
			  "src/navigation/*"
		  ],
		  "@screens/*": [
			  "src/screens/*"
		  ],
		  "@components/*": [
			  "src/components/*"
		  ],
		  "@theme": [
			  "src/theme/index"
		  ],
		  "@api/*": [
			  "src/api/*"
		  ],
		  "@assets/*": [
			  "src/assets/*"
		  ],
		  "@redux/*": [
			  "src/redux/*"
		  ],
	  },
  }
  }
  